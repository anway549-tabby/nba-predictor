  "version": "1.0.0",
  "description": "NBA Player Props Prediction Platform - Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "backfill": "ts-node src/services/ingestion/backfillESPN.ts",
    "backfill:old": "ts-node src/services/ingestion/backfillLast15Games.ts",
    "refresh:daily": "ts-node src/services/ingestion/dailyRefreshESPN.ts",
    "refresh:old": "ts-node src/services/ingestion/dailyRefreshJob.ts",
    "test:espn": "ts-node src/services/ingestion/testEspnApi.ts",
    "test:schedule": "ts-node src/services/ingestion/testFreeApi.ts"
  },
  "keywords": [
    "nba",
    "predictions",
    "api"
  ],
  "author": "",
  "license": "ISC",
